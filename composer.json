{
  "name": "sloths4/dodomudojade",
  "description": "This is DoDomuDojadÄ™ app repository.",
  "require": {
    "php": "^8.5",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "ext-readline": "*",
    "ext-simplexml": "*",
    "google/apiclient": "^2.18",
    "guzzlehttp/psr7": "^2.6",
    "monolog/monolog": "^3.10.0",
    "nikic/fast-route": "^1.3",
    "phpmailer/phpmailer": "^6.12",
    "psr/http-message": "^1.1",
    "psr/log": "^3.0",
    "symfony/http-client": "^7.4.3",
    "twig/twig": "^3.0",
    "vlucas/phpdotenv": "^5.6"
  },
  "require-dev": {
    "jetbrains/phpstorm-attributes": "^1.2",
    "phpdocumentor/shim": "^3.9",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^12.5",
    "roave/security-advisories": "dev-latest",
    "symfony/var-dumper": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "App\\Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "8.5.0"
    },
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "phpdocumentor/shim": true,
      "roave/security-advisories": true
    }
  },
  "license": "CC-BY-NC-4.0"
}
