{
  "name": "sloths4/dodomudojade",
  "description": "This is DoDomuDojadÄ™ app repository.",
  "require": {
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-mbstring": "*",
    "ext-simplexml": "*",
    "ext-libxml": "*",
    "ext-sqlite3": "*",
    "php": "^8.4",
    "monolog/monolog": "^3.0",
    "symfony/http-client": "^7.4",
    "vlucas/phpdotenv": "^5.6",
    "nikic/fast-route": "^1.3",
    "phpmailer/phpmailer": "^6.12"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.4",
    "jetbrains/phpstorm-attributes": "^1.2",
    "ext-readline": "*",
    "phpstan/phpstan": "^2.1",
    "roave/security-advisories": "dev-latest",
    "symfony/var-dumper": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "App\\Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "8.4.0"
    },
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "roave/security-advisories": true
    }
  },
  "license": "CC-BY-NC-4.0"
}
